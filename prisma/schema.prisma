generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Games {
  Id             String    @id(map: "PK__Games__3214EC0785D8E360") @db.UniqueIdentifier
  title          String    @db.NVarChar(200)
  description    String    @db.Text
  imageUrl       String    @db.NVarChar(200)
  leader         String    @db.NVarChar(200)
  maxPlayerCount Int
  schedule       String    @db.NVarChar(200)
  system         String    @db.NVarChar(200)
  createdAt      DateTime  @db.DateTime
  updatedAt      DateTime? @db.DateTime
}
